<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="register_list_view" model="ir.ui.view">
        <field name="name">register.list.view</field>
        <field name="model">registration.api</field>
        <field name="arch" type="xml">
            <tree string="Register Api">
                    <field name="email_id"/>
                    <field name="password"/>
                    <field name="mobile_number"/>
                    <field name="Addrese"/>
            </tree>
        </field>
    </record>
    <record id="register_form_view" model="ir.ui.view">
        <field name="name">register.form.view</field>
        <field name="model">registration.api</field>
        <field name="arch" type="xml">
            <form string="Website">
                <sheet>
                    <group>
                        <group>
                            <field name="email_id"/>
                            <field name="password"/>
                            <field name="mobile_number"/>
                            <field name="Addrese"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_register_agent" model="ir.actions.act_window">
        <field name="name">Registration</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">registration.api</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face"> Create a new record! </p>
        </field>
    </record>

    <menuitem id="agent_register_menu" action="action_register_agent" parent="agent_web_menu" name="Registration"/>

       
    
        
        

         
</odoo>